{
  "expo": {
    "name": "Lykkebo",
    "slug": "Lykkebo",
    "privacy": "public",
    "icon": "./app-icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.1.6",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dk.malerfirmaet-lykkebo"
    },
    "android": {
      "package": "com.lykkebo.lykebbo",
      "permissions": [ "CAMERA" ],
      "versionCode": 16
    }
  }
}
